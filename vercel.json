{
    "builds":  [
                   {
                       "src":  "api/index.js",
                       "use":  "@vercel/node"
                   },
                   {
                       "config":  {
                                      "distDir":  "dist"
                                  },
                       "use":  "@vercel/static-build",
                       "src":  "package.json"
                   }
               ],
    "routes":  [
                   {
                       "src":  "/api/(.*)",
                       "dest":  "/api/index.js"
                   },
                   {
                       "src":  "/health",
                       "dest":  "/api/index.js"
                   },
                   {
                       "handle":  "filesystem"
                   },
                   {
                       "src":  "/(.*)",
                       "dest":  "/index.html"
                   }
               ],
    "version":  2
}